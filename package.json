{
  "name": "orbitools-plugin",
  "version": "1.0.1",
  "description": "WordPress Orbitools Plugin with Layout Blocks",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run build:blocks && npm run build:assets",
    "build:blocks": "wp-scripts build --config webpack.blocks.js",
    "build:assets": "webpack --config webpack.assets.js",
    "start": "wp-scripts start --config webpack.blocks.js",
    "start:blocks": "wp-scripts start --config webpack.blocks.js",
    "start:assets": "webpack --config webpack.assets.js --watch",
    "dev": "npm-run-all --parallel start:blocks start:assets",
    "bump:patch": "./scripts/bump-version.sh patch",
    "bump:minor": "./scripts/bump-version.sh minor",
    "bump:major": "./scripts/bump-version.sh major",
    "release": "./scripts/build-release.sh",
    "release:patch": "./scripts/bump-version.sh patch && ./scripts/build-release.sh",
    "release:minor": "./scripts/bump-version.sh minor && ./scripts/build-release.sh",
    "release:major": "./scripts/bump-version.sh major && ./scripts/build-release.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@types/react": "^18.0.0",
    "@types/wordpress__block-editor": "^11.0.0",
    "@types/wordpress__blocks": "^12.0.0",
    "@types/wordpress__components": "^23.0.0",
    "@wordpress/scripts": "^30.20.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "sass-loader": "^13.3.2",
    "typescript": "^5.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@wordpress/block-editor": "^12.0.0",
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^25.0.0",
    "@wordpress/element": "^5.0.0",
    "@wordpress/i18n": "^4.0.0"
  }
}
